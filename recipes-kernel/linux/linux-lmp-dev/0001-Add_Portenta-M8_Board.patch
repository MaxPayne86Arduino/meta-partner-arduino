+
+  analogix_i2c@58{
+    compatible ="analogix,anx7625";
+    status = "okay";
+    reg = <0x58>;
+
+    interrupt-parent = <&gpio4>;
+    interrupts = <23 0>, <22 0>; // cable_det gpio is interrupt pin
+    p_on-gpios = <&gpio4 21 0x0>;       // VIDEO_EN
