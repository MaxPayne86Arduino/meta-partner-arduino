+    analogix.external-ldo-control = <0>;
+    analogix,i2c-pull-up = <0>;
+    port {
+      lvds_from_dsi: endpoint {
+        remote-endpoint = <&dsi_to_lvds>;
+      };
+    };
+  };
+};
