FB: ucmd setenv earlycon "ec_imx6q,0x30890000,115200"
FB: ucmd setenv bootargs console=${console},${baudrate} earlycon=${earlycon},${baudrate} rdinit=/linuxrc
SDPV: delay 1000

#FB: acmd booti ${loadaddr} - ${fdt_addr}
FB: acmd bootm ${loadaddr}
