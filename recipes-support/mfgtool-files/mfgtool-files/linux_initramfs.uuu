uuu_version 0.64.43

SDP: boot -f imx-boot-mfgtool

SDPV: delay 1000
SDPV: write -f u-boot-mfgtool.itb
SDPV: jump

FB: ucmd setenv fastboot_buffer ${loadaddr}

FB: download -f fitImage

#FB: ucmd setenv fastboot_buffer ${fdt_addr}
